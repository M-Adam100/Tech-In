{
	"name": "Tech-In",
	"author": "earthragellc",
	"version": "1.0.1",
	"description": "",
	"manifest_version": 3,
	"incognito": "split",
	"icons": {
		"16": "icons/16.png",
		"32": "icons/32.png",
		"64": "icons/64.png",
		"128": "icons/128.png"
